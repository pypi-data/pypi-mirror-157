{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "description": "Metadata of a crawler run",
    "properties": {
        "crawlerName": {
            "default": "",
            "type": "string"
        },
        "endTime": {
            "format": "date-time",
            "type": "string"
        },
        "entityCount": {
            "default": 0,
            "type": "number"
        },
        "startTime": {
            "format": "date-time",
            "type": "string"
        },
        "status": {
            "enum": [
                "FAILURE",
                "PENDING",
                "SUCCESS"
            ],
            "type": "string"
        }
    },
    "type": "object"
}

