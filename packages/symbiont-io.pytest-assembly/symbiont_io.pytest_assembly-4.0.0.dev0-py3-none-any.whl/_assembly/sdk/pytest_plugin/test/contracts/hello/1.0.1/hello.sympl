@clientside
def hello_world(name: str='Alice') -> str:
    message = f'Hello {name}!'
    if name == "Count Rugen":
        message = "Hello. My name is Inigo Montoya. You killed my father. Prepare to die."
    return message


@clientside
def other_world(name: str='Alice') -> str:
    message = f'Hello, {name}! This is the other world.'
    return message
