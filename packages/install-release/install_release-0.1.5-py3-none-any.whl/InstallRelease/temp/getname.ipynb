    '    platform.system(),\n',
    ')\n',
    '\n',
    'file_names = []\n',
    'with open(\'./names.txt\', \'r\') as names:\n',
    '    line = names.readline()\n',
    '    while line:\n',
    '        if line.isascii():\n',
    '            file_names.append(line.strip())\n',
    '        line = names.readline()\n',
    '\n'
   ]
